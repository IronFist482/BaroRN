{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": ".",
    "paths": {
      "": ["src/"],
      "tests": ["tests/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["./src/utils/*"],
      "@elements/*": ["./src/elements/*"],
      "@assets/*": ["./src/assets/*"],
      "@screens/*": ["./src/screens/*"],
      "@api/*": ["./src/api/*"],
      "@config/*": ["./src/config/*"],
      "@navigation/*": ["./src/navigation/*"],
      "@store/*": ["./src/store/*"],
      "@fonts/*": ["./src/fonts/*"],
      "@hooks/*": ["./src/hooks/*"]
    }
  }
}
